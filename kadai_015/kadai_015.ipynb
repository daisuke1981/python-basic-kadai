        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前は{self.name}年齢は{self.age}\")\n",
        "\n",
        "\n",
        "human = Human(\"熊谷大介\", 43)\n",
        "human.printinfo()\n"
      ]
